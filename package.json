{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "dev:client": "EXPO_NO_TELEMETRY=1 expo start --dev-client",
    "build:web": "expo export --platform web",
    "build:ios:sim": "eas build --profile development --platform ios",
    "build:ios:device": "eas build --profile development-device --platform ios",
    "build:ios:preview": "eas build --profile preview --platform ios",
    "build:ios:prod": "eas build --profile production --platform ios",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
