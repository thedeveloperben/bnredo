    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "playwright": "^1.58.0",
    "react-test-renderer": "^19.1.0",
